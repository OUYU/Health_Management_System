@model AIPProject01.Models.ViewModel.G3_User

@{
    ViewBag.Title = "身心健康與安全管理系統";
}

@section featured {
    <section class="featured">
        <div class="content-wrapper">
            <hgroup class="title">
                <h1>@ViewBag.Title</h1>
                <h2>@ViewBag.Message</h2>
            </hgroup>
        </div>
    </section>
}

<dt style='font-size:30px;'>您的ID :</dt>
<dd style='font-size:30px;'>@Model.ID</dd>

<dt style='font-size:30px;'>您的帳號 :</dt>
<dd style='font-size:30px;'>@Model.NAME</dd>

<dt style='font-size:30px;'>您的密碼 :</dt>
<dd style='font-size:30px;'>@Model.PASSWORD</dd>

<dt style='font-size:30px;'>您的性別 :</dt>
<dd style='font-size:30px;'>@Model.SEX</dd>